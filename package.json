{
  "name": "@forlagshuset/nestjs-mongoose-paginate",
  "version": "1.2.7",
  "description": "A pagination, filtering and sorting lib for nestjs using mongoose orm",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fagbokforlaget/nestjs-mongoose-paginate.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fagbokforlaget/nestjs-mongoose-paginate/issues"
  },
  "homepage": "https://github.com/fagbokforlaget/nestjs-mongoose-paginate#readme",
  "devDependencies": {
    "@nestjs/testing": "^10.2.6",
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "ajv": "^8.12.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.2.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "@nestjs/swagger": "^7.1.12",
    "@nestjs/core": "^10.2.6",
    "@nestjs/platform-express": "^10.2.6",
    "rxjs": "^7.8.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "setupFiles": [
      "./jest-setup-file.ts"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "^.+\\.js$"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
